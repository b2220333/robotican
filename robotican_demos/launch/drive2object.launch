<?xml version="1.0"?>
<launch>
  
  
  <!-- roslaunch robotican_armadillo armadillo.launch lidar:=true kinect2:=true gmapping:=true move_base:=true world_name:="`rospack find robotican_common`/worlds/objects_on_table.world" gazebo:=true -->

<!-- roslaunch robotican_armadillo armadillo.launch lidar:=true kinect2:=true gmapping:=true move_base:=true -->
  
  
  <node name="find_object_node" pkg="robotican_common" type="find_objects_node" output="screen">
        <param name="~object_r" value="0.025"/>
    <param name="~object_h" value="0.15"/>
    <param name="~object_id" value="can"/>
    <param name="~depth_topic" value="/kinect2/qhd/points"/>
    <!-- <param name="~depth_topic" value="/depth_camera/depth_registered/points"/> -->
    <rosparam file="$(find robotican_demos)/config/red_object.yaml" command="load"/>
  </node>  
  
  <node name="drive2object_node" pkg="robotican_common" type="drive2object_node" output="screen">
    <param name="~object_id" value="can"/>
 <param name="~base_distance_from_object" value="0.55"/>
  </node>  
  
  <node name="object_view" pkg="rqt_image_view" type="rqt_image_view" >
    <remap from="image" to="find_object_node/result" />
  </node>
  
  
  
  
</launch>


<launch>
	<rosparam file="$(find robotican_hardware_interface)/config/arm_config.yaml" command="load"/>
	<rosparam file="$(find robotican_hardware_interface)/config/controllers.yaml" command="load"/>
	<param name="robot_description" command="$(find xacro)/xacro.py '$(find robotican_armadillo)/description/models/armadillo.xacro'" />
	<node name="arm_manager" pkg="robotican_hardware_interface" type="dynamixel_node" respawn="false" output="screen" />
	<node name="dxl_test" pkg="robotican_hardware_interface" type="dynamixel_motor_test_node" respawn="false" output="screen" />
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="true" respawn_delay="5" output="screen" args="robot_state_controller torso_position_controller" />
</launch>

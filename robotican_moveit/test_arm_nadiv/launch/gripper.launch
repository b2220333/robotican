<launch>
	<rosparam file="$(find test_arm_nadiv)/config/gripper.yaml" command="load"/>
	<rosparam file="$(find test_arm_nadiv)/config/gripper_action_server.yaml" command="load"/>
    <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                arm_port:
                    port_name: "/dev/USB2DYNAMIXEL_GRIPPER"
                    baud_rate: 2000000
                    min_motor_id: 1
                    max_motor_id: 2
                    update_rate: 20
        </rosparam>
    </node>
    
      <node name="arm_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port=arm_port
		left_finger_controller
		right_finger_controller"
				output="screen"/>
				
     <node name="gripper_controller_spawner_meta" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --type=meta
                gripper_controller
		left_finger_controller
		right_finger_controller"
                output="screen"/>
	  
</launch>
